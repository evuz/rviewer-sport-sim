{
  "name": "sport-simulator",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run serve",
    "dev": "npm run build && run-p build:watch serve:watch",
    "serve": "node dist/index.js",
    "serve:watch": "node --watch dist/index.js --log-watch-compilation",
    "build": "swc ./src -d dist",
    "build:watch": "swc ./src -d dist --watch",
    "type:check": "tsc src/index.ts --noEmit",
    "type:check:watch": "tsc src/index.ts --noEmit --watch",
    "lint": "eslint \"**/src/**/*.{ts,tsx}\"",
    "lint:fix": "prettier --write \"**/src/**/*.{ts,tsx}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "eslint-config-standard": "^17.1.0",
    "express": "^4.18.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2"
  }
}
